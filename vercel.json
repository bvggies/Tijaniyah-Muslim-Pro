{
  "version": 2,
  "builds": [
    {
      "src": "public/index.php",
      "use": "@vercel/php"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "public/index.php"
    }
  ],
  "env": {
    "APP_ENV": "production",
    "APP_DEBUG": "false",
    "APP_URL": "https://tijaniyah-muslim-pro.vercel.app"
  },
  "functions": {
    "public/index.php": {
      "maxDuration": 30
    }
  }
}
